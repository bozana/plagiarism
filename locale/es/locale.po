msgid "plugins.generic.plagiarism.manager.settings.username"
msgstr "Nombre de usuario/a de iThenticate"

msgid "plugins.generic.plagiarism.manager.settings.password"
msgstr "Contraseña de iThenticate"

msgid "plugins.generic.plagiarism.manager.settings.usernameRequired"
msgstr "El nombre de usuario/a de iThenticate es obligatorio"

msgid "plugins.generic.plagiarism.manager.settings.passwordRequired"
msgstr "La contraseña de iThenticate es obligatoria"

msgid "plugins.generic.plagiarism.manager.settings.areForced"
msgstr ""
"Se han encontrado los ajustes de iThenticate en config.inc.php y, por tanto, "
"no se utilizarán estos ajustes."

msgid "plugins.generic.plagiarism.errorMessage"
msgstr ""
"La carga del envío {$submissionId} a iThenticate ha fallado con el error: "
"{$errorMessage}"
